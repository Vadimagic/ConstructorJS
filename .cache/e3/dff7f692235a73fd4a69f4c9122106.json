"classes/blocks.js":"import {row, col, css} from \"../utils\"\r\n\r\nexport class Block {\r\n\tconstructor(type, value, options) {\r\n\t\tthis.type = type\r\n\t\tthis.value = value\r\n\t\tthis.options = options\r\n\t}\r\n\r\n\ttoHTML() {\r\n\t\tthrow new Error('Метод toHTML должен быть реализован!')\r\n\t}\r\n}\r\n\r\nexport class Title extends Block {\r\n\tconstructor(value, options) {\r\n\t\tsuper('title', value, options)\r\n\t}\r\n\r\n\ttoHTML() {\r\n\t\tconst {tag = 'h1', styles} = this.options\r\n\t\treturn row(col(`<${tag}>${this.value}</${tag}>`), css(styles))\r\n\t}\r\n}\r\n\r\nexport class Image extends Block {\r\n\tconstructor(value, options) {\r\n\t\tsuper('image', value, options)\r\n\t}\r\n\r\n\ttoHTML() {\r\n\t\tconst {styles, imageStyles : is, alt = \"\"} = this.options\r\n\t\treturn row(`<img src=\"${this.value}\" alt=\"${alt}\" style=\"${css(is)}\"`, css(styles))\r\n\t}\r\n}\r\n\r\nexport class Columns extends Block {\r\n\tconstructor(value, options) {\r\n\t\tsuper('columns', value, options)\r\n\t}\r\n\r\n\ttoHTML() {\r\n\t\tconst {styles} = this.options\r\n\t\tconst html = this.value.map(col).join(\"\") // e=> col(e) == col\r\n\t\treturn row(html, css(styles))\r\n\t}\r\n}\r\n\r\nexport class Text extends Block {\r\n\tconstructor(value, options) {\r\n\t\tsuper('text', value, options)\r\n\t}\r\n\r\n\ttoHTML() {\r\n\t\tconst {styles} = this.options\r\n\t\treturn row(col(`<p>${this.value}</p>`), css(styles))\r\n\t}\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"470d1d5189908ca2351355768105c11f","cacheData":{"env":{}}}